set viminfo="NONE"
set number
set nowrap
set laststatus=2
set statusline+=%F
set ignorecase
set smartindent
set tabstop=4
set shiftwidth=4
syntax on
hi Normal guibg=NONE ctermbg=NONE
hi LineNr ctermfg=NONE ctermbg=NONE
hi StatusLine cterm=bold ctermbg=NONE ctermfg=magenta
let g:netrw_dirhistmax = 0
